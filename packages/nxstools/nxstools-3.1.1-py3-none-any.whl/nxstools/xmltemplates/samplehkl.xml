<?xml version='1.0'?>
<definition>
  <group type="NXentry" name="$var.entryname#'$(__entryname__)'$var.serialno">
    <group type="NXsample" name="$(sname)">
      <field type="NX_FLOAT64" name="h">
      <strategy mode="INIT"/>$datasources.$(h)</field>
      <field type="NX_FLOAT64" name="k">
      <strategy mode="INIT"/>$datasources.$(k)</field>
      <field type="NX_FLOAT64" name="l">
      <strategy mode="INIT"/>$datasources.$(l)</field>
      <field units="deg" type="NX_FLOAT64" name="psi">
      <strategy mode="INIT"/>$datasources.$(psi)</field>
    </group>
  </group>
</definition>
