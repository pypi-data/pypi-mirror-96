nxscreate clientds title \-d /home/user/xmldir
nxscreate clientds \-v exp_c \-f1 \-l4 \-b
nxscreate clientds \-v hasppXX:10000/expchan/vfcadc_exp/ \-f1 \-l8  \-m \-b \-s exp_vfc
