<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:lx="lixi.org.au/schema/appinfo_elements" xmlns:li="lixi.org.au/schema/appinfo_instructions" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:element name="Customisations">
		<xs:annotation>
			<xs:documentation>The set of customisations (restrictions and custom documentation) that apply in order to generate a valid LIXI Customisation by Restriction Instruction file.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CustomiseItem" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>The customisations that apply to a single item, uniquely identified by the LIXI path.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Path" type="xs:string">
								<xs:annotation>
									<xs:documentation>The LIXI path, that identifies the item to customise.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="CustomExcerpt" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The custom excerpt for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="CustomDocumentation" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The custom documentation for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Attribute" type="xs:string" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The custom attribute to keep in include.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Element" type="xs:string" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The custom element to keep in include.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="CustomMaxOccurs" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The custom maxOccurs for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="CustomMinOccurs" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The custom minOccurs for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Enumeration" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The custom enumeration for the item.</xs:documentation>
								</xs:annotation>
								<xs:complexType mixed="true">
									<xs:attribute name="Definition" type="xs:string" use="optional">
										<xs:annotation>
											<xs:documentation>Definition of the custom enumeration.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Pattern" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The custom pattern for the item.</xs:documentation>
								</xs:annotation>
								<xs:complexType mixed="true">
									<xs:attribute name="Definition" type="xs:string" use="optional">
										<xs:annotation>
											<xs:documentation>Definition of the custom pattern.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="MinCurrency" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The minimum range for the item.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:pattern value="\d*\.\d{2}"/>
										<xs:minInclusive value="0" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="MaxCurrency" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The maximum range for the item.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:pattern value="\d*\.\d{2}"/>
										<xs:minInclusive value="0" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="MinDateTime" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The minimum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MaxDateTime" type="xs:dateTime" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The maximum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MinDate" type="xs:date" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The minimum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MaxDate" type="xs:date" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The maximum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MinDecimal" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The minimum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MaxDecimal" type="xs:decimal" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The maximum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MinYear" type="xs:gYear" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The minimum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MaxYear" type="xs:gYear" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The maximum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MinInteger" type="xs:integer" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The minimum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MaxInteger" type="xs:integer" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The maximum range for the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MinPercentage" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The minimum range for the item.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:pattern value="\d*\.\d{2}"/>
										<xs:maxInclusive value="99" />
										<xs:minInclusive value="0" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="MaxPercentage" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The maximum range for the item.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:pattern value="\d*\.\d{2}"/>
										<xs:maxInclusive value="100" />
										<xs:minInclusive value="1" />
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="Include" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A element that is included has attributes other than specified attributes explicitly excluded from the customised schema.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Whitelist" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A element that is included has attributes other than specified attributes explicitly excluded from the customised schema.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Exclude" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A item that is excluded is explicitly removed from the customised schema.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Blacklist" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A element that is included has attributes other than specified attributes explicitly excluded from the customised schema.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ExcludeAllElements" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A flag to indicate all child elements are to be excluded from the customised schema in a include instruction.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="BlacklistAllElements" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A flag to indicate all child elements are to be excluded from the customised schema in a include instruction.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="ExcludeAllAttributes" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A flag to indicate all child attributes are to be excluded from the customised schema in a include instruction.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="BlacklistAllAttributes" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A flag to indicate all child attributes are to be excluded from the customised schema in a include instruction.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="OptionalToMandatory" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A item that is explicitly made mandatory switched to use="required" (for attributes) or minOccurs="1" (for elements).</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="StringToList" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A item that is explicitly given a set of enumerations to choose from.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="StringToPattern" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A item that is explicitly given a pattern to conform to.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomCurrencyRange" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>An currency item that has its minimum and/or maximum range explicitly set.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomDateTimeRange" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>An date item item that has its minimum and/or maximum range explicitly set.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomDateRange" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>An date item that has its minimum and/or maximum range explicitly set.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomDecimalRange" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>An decimal item that has its minimum and/or maximum range explicitly set.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomYearRange" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>An year item that has its minimum and/or maximum range explicitly set.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomIntegerRange" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>An integer item that has its minimum and/or maximum range explicitly set.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomPercentageRange" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>An percentage item that has its minimum and/or maximum range explicitly set.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomMaxOccurs" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A item that has its maxOccurs explicitly set.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomMinOccurs" type="xs:string" use="optional" fixed="Yes">
							<xs:annotation>
								<xs:documentation>A item that has its minOccurs explicitly set.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="CustomHeading" type="xs:string" use="optional">
							<xs:annotation>
								<xs:documentation>The custom documentation heading for the item (overrides top level value).</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="customHeading" type="xs:string" use="optional">
							<xs:annotation>
								<xs:documentation>The custom documentation heading for the item (overrides top level value).</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="LIXICode" type="xs:string">
				<xs:annotation>
					<xs:documentation>The LIXI Code of the organisation that is customising the schema.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CustomisationCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>The organisation that is customising the schema creates this code for uniquely identifying this customisation.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="LIXITransactionType" type="xs:string">
				<xs:annotation>
					<xs:documentation>The LIXI Transaction Type that is used as the basis for this customisation.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CustomHeading" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The custom documentation heading for the item (This overrides lower level  value).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="customHeading" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>The custom documentation heading for the item (This overrides lower level  value).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
</xs:schema>