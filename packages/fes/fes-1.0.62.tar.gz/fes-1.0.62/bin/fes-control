#!/usr/bin/env python3

__version__ = '1.0.62'

from pathlib import Path
from pyaltt2.app import manage_gunicorn_app

dir_me = Path(__file__).absolute().parents[1]

manage_gunicorn_app('fes',
                    dir_me,
                    name='FES',
                    version=__version__,
                    default_port=8832,
                    api_uri='/jrpc')
