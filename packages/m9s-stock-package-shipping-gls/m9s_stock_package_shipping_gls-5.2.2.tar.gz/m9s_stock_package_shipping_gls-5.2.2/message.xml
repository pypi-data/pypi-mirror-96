<tryton>
    <data group="1">
        <record model="ir.message" id="warehouse_address_required">
            <field name="text">An address is required for warehouse "%(warehouse)s".</field>
        </record>
        <record model="ir.message" id="phone_required">
            <field name="text">A phone number is required for party "%(party)s".</field>
        </record>
        <record model="ir.message" id="shipping_description_required">
            <field name="text">A shipping description is required for shipment "%(shipment)s".</field>
        </record>
        <record model="ir.message" id="missing_configuration">
            <field name="text">Missing configuration for company "%(company)s".</field>
        </record>
        <record model="ir.message" id="gls_webservice_error">
            <field name="text">GLS webservice call failed with the following error message:\n\n%(message)s</field>
        </record>
        <record model="ir.message" id="has_reference_number">
            <field name="text">Shipment "%(shipment)s" already has a reference number.</field>
        </record>
        <record model="ir.message" id="email_or_mobile_required">
            <field name="text">The Service chosen for shipment "%(shipment)s" requires either an email or a mobile number of the recipient.</field>
        </record>
    </data>
</tryton>
