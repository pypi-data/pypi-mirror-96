msgctxt "error:stock.shipment.create_shipping.gls:"
msgid ""
"GLS webservice call failed with the following error message:\n"
"\n"
"%(message)s"
msgstr ""
"Der Aufruf des GLS Webservice schlug fehl mit der folgenden Fehlermeldung:\n"
"\n"
"%(message)s"

msgctxt "error:stock.shipment.create_shipping.gls:"
msgid "Missing configuration  for company \"%(company)s\"."
msgstr "Das Unternehmen \"%(company)s\" ist nicht für den Versand konfiguriert."

msgctxt "error:stock.shipment.create_shipping.gls:"
msgid "Shipment \"%(shipment)s\" already has a reference number."
msgstr "Lieferposten \"%(shipment)s\" hat bereits eine Belegnummer."

msgctxt "error:stock.shipment.create_shipping.gls:"
msgid ""
"The Service chosen for shipment \"%(shipment)s\" requires either an email or"
" a mobile number of the recipient."
msgstr ""
"Der für den Lieferposten \"%(shipment)s\" gewählte Dienst benötigt entweder "
"eine E-Mail-Adresse oder eine Mobilnummer des Empfängers."

msgctxt "error:stock.shipment.out:"
msgid "A phone number is required for party \"%(party)s\"."
msgstr "Fehlende Telefonnummer für Partei \"%(party)s\"."

msgctxt "error:stock.shipment.out:"
msgid "A shipping description is required for shipment \"%(shipment)s\"."
msgstr "Fehlende Beschreibung für Lieferposten \"%(shipment)s\"."

msgctxt "error:stock.shipment.out:"
msgid "An address is required for warehouse \"%(warehouse)s\"."
msgstr "Fehlende Adresse für Warenlager \"%(warehouse)s\"."

