                        <property name="can-focus">1</property>
                        <property name="editable">0</property>
                        <property name="cursor-visible">0</property>
                        <property name="pixels-above-lines">1</property>
                        <property name="pixels-below-lines">1</property>
                        <property name="left-margin">10</property>
                        <property name="right-margin">10</property>
                        <property name="top-margin">5</property>
                        <property name="bottom-margin">5</property>
                        <signal name="button-press-event" handler="on_log_window_clicked" swapped="no"/>
                        <signal name="popup-menu" handler="on_popup_log_menu" swapped="no"/>
                        <signal name="touch-event" handler="on_log_window_clicked" swapped="no"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="resize">0</property>
                <property name="shrink">0</property>
              </packing>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkSeparator">
            <property name="visible">1</property>
          </object>
        </child>
        <!-- Status Bar -->
        <child>
          <object class="GtkBox">
            <property name="visible">1</property>
            <property name="margin-start">2</property>
            <property name="margin-end">2</property>
            <property name="margin-top">2</property>
            <property name="margin-bottom">2</property>
            <child>
              <object class="GtkLabel" id="Statusbar">
                <property name="visible">1</property>
                <property name="halign">start</property>
                <property name="hexpand">1</property>
                <property name="margin-start">10</property>
                <property name="margin-top">0</property>
                <property name="margin-bottom">0</property>
                <property name="ellipsize">end</property>
                <property name="single-line-mode">1</property>
              </object>
            </child>
            <child>
              <object class="GtkProgressBar" id="SharesProgress">
                <property name="valign">center</property>
                <property name="pulse-step">0.10000000149</property>
                <property name="text" translatable="yes">Scanning Shares</property>
                <property name="show-text">1</property>
              </object>
            </child>
            <child>
              <object class="GtkProgressBar" id="BuddySharesProgress">
                <property name="valign">center</property>
                <property name="pulse-step">0.10000000149</property>
                <property name="margin-start">18</property>
                <property name="text" translatable="yes">Scanning Buddy Shares</property>
                <property name="show-text">1</property>
              </object>
            </child>
            <child>
              <object class="GtkImage">
                <property name="visible">1</property>
                <property name="icon-name">network-wireless-symbolic</property>
                <property name="margin-start">20</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="SocketStatus">
                <property name="visible">1</property>
                <property name="margin-start">8</property>
                <property name="margin-top">0</property>
                <property name="margin-bottom">0</property>
                <property name="label">0/0</property>
                <property name="tooltip-text" translatable="yes">Connections</property>
              </object>
            </child>
            <child>
              <object class="GtkImage">
                <property name="visible">1</property>
                <property name="icon-name">go-down-symbolic</property>
                <property name="margin-start">20</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="DownStatus">
                <property name="visible">1</property>
                <property name="margin-start">8</property>
                <property name="margin-top">0</property>
                <property name="margin-bottom">0</property>
                <property name="tooltip-text" translatable="yes">Downloading (speed / active downloads)</property>
              </object>
            </child>
            <child>
              <object class="GtkImage">
                <property name="visible">1</property>
                <property name="icon-name">go-up-symbolic</property>
                <property name="margin-start">20</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="UpStatus">
                <property name="visible">1</property>
                <property name="margin-start">8</property>
                <property name="margin-top">0</property>
                <property name="margin-bottom">0</property>
                <property name="tooltip-text" translatable="yes">Uploading (speed / active uploads)</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="UserStatus">
                <property name="visible">1</property>
                <property name="margin-start">25</property>
                <property name="margin-end">10</property>
                <property name="margin-top">0</property>
                <property name="margin-bottom">0</property>
                <property name="label">Offline</property>
                <attributes>
                  <attribute name="weight" value="bold"/>
                </attributes>
              </object>
            </child>
            <child>
              <object class="GtkButton">
                <property name="visible">1</property>
                <property name="can-focus">1</property>
                <property name="tooltip-text" translatable="yes">Show log history</property>
                <property name="action-name">win.showlog</property>
                <child>
                  <object class="GtkImage">
                    <property name="visible">1</property>
                    <property name="icon-name">view-more-symbolic</property>
                  </object>
                </child>
                <style>
                  <class name="circular"/>
                  <class name="flat"/>
                  <class name="image-button"/>
                </style>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
