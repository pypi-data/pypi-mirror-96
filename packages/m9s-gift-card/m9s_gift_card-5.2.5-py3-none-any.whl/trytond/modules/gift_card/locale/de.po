msgctxt "model:ir.message,text:amounts_out_of_range"
msgid ""
"Gift card amount must be within %(gc_min)s %(currency_code)s and %(gc_max)s "
"%(currency_code)s."
msgstr ""
"Gutscheinbetrag muss zwischen %(gc_min)s %(currency_code)s und %(gc_max)s "
"%(currency_code)s liegen."

msgctxt "model:ir.message,text:deletion_not_allowed"
msgid "Gift cards can not be deleted in active state."
msgstr "Gutscheine können nicht gelöscht werden so lang sie aktiv sind."

msgctxt "model:ir.message,text:gift_card_inactive"
msgid "The gift card to be redeemed must be in active state."
msgstr "Der einzulösende Gutschein muss aktiv sein."

msgctxt "model:ir.message,text:gift_card_paid_message"
msgid "Paid by Gift Card (%(number)s)"
msgstr "Zahlung mit Gutschein (%(number)s)"

msgctxt "model:ir.message,text:gift_card_redeemed"
msgid "This gift card has already been redeemed."
msgstr "Dieser Gutschein wurde bereits eingelöst"

msgctxt "model:ir.message,text:inappropriate_product_type"
msgid "The product type of %s must be service for gift cards."
msgstr "Der Artikeltyp von %s muss Dienstleistung sein für Gutschein."

msgctxt "model:ir.message,text:insufficient_amount"
msgid "Card %s is found to have insufficient amount."
msgstr "Gutschein %s weist nicht genügend Deckung auf."

msgctxt "model:ir.message,text:insufficient_amount_to_pay"
msgid "Gift card %s has no sufficient amount to pay %s %s."
msgstr ""
"Gutschein %s weist nicht genügend Deckung auf für die Zahlung von %s %s."

msgctxt "model:ir.message,text:invalid_amount"
msgid ""
"Gift Card minimum amount must be smaller than gift card maximum amount."
msgstr "Minimalbetrag muss kleiner als Maximalbetrag sein."

msgctxt "model:ir.message,text:missing_liability_account"
msgid "Liability Account is missing from Gift Card Configuration."
msgstr ""
"Verbindlichkeitskonto ist in den Gutscheineinstellungen nicht konfiguriert."

msgctxt "model:ir.message,text:multiple_giftcards"
msgid "You can only redeem a single gift card at a time."
msgstr "Es kann nur jeweils ein Gutschein eingelöst werden."

msgctxt "model:ir.message,text:negative_amount"
msgid "The amount to be entered cannot be negative."
msgstr "Es können nur positive Beträge eingegeben werden."

msgctxt "model:ir.message,text:negative_amount_not_allowed"
msgid "Gift card amounts can not be negative."
msgstr "Gutscheinbeträge können nur positiv sein."

msgctxt "model:ir.message,text:redeem_done_message"
msgid ""
"The gift card was redeemed with %(amount)s %(code)s captured in this "
"transaction, %(captured)s %(code)s captured overall and %(remaining)s "
"%(cod)s remaining."
msgstr ""
"Der Gutschein wurde eingelöst mit %(amount)s %(code)s eingezogen in dieser "
"Transaktion, %(captured)s %(code)s eingezogen gesamt und %(remaining)s "
"%(cod)s Restbetrag verbleibend."

