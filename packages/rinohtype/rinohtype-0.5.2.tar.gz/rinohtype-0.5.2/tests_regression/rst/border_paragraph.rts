[STYLESHEET]
base = sphinx_base14


[flowable with fat border : Flowable(has_class='fat-border')]
border = 20pt, #000
padding = 5pt
horizontal_align = center


[spacer : Paragraph(has_class='spacer')]
border = 1pt, #555
padding_top = 6cm
padding_bottom = 6cm
text_align = center


[small spacer : Paragraph(has_class='small-spacer')]
base = spacer
padding_top = 1cm
padding_bottom = 1cm
